@using IQMedia.Model;
@model List<TopResults>
@if (Model != null)
{
    if (Model.Count > 0)
    {
    <table class="topResult" width="100%">
        @foreach (TopResults topResults in Model)
        {
            <tr>
                <td style="width:40px;">
                    <img src="@topResults.Logo" alt="" />
                </td>
                <td>@topResults.Title
                </td>
                <td>@Html.Raw(@topResults.Publisher)
                </td>
            </tr>
        }
    </table>
    }

}