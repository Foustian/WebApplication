<script type="text/javascript">
    $(function () {
        _PlayState = 1;
        _firstCall = true;
        _SearchTerm = '@Convert.ToString(Request.QueryString["SearchTerm"])';
        _IQAgentID = '@Convert.ToInt64(Request.QueryString["IQAgentID"])';
        _SeqID = '@Convert.ToInt64(Request.QueryString["SeqID"])';
        if (_IQAgentID > 0) {
            LoadPlayerbyAgentID(_IQAgentID, AppendPlayer);
        }
        else if (_SeqID > 0) {
            LoadPlayerbyAgentID(_SeqID, AppendPlayer);
        }
        else {
            LoadPlayerbyGuidTS(_ID, '', _SearchTerm, false, AppendPlayer);
        }
        if ($("#divCapHighlightContent").text().trim().length > 0) {
            ChangeTabPlayer(0);
        }
    });

    var AppendPlayer = function () {

        $("#divPlayer").removeClass("video-modalPlayer");
        $("#divPlayer").removeClass("hide");
        $("#divPlayer").detach().appendTo("#divClipPlayer");

    }

    function OnFail(result) {
        _IsTabChange = false;
        ShowNotification(_msgErrorOccured);
    }
</script>
