@model List<IQMedia.Model.IQ_ReportModel>
@if (Model != null)
{
    if (Model.Count > 0)
    {
        foreach (IQMedia.Model.IQ_ReportModel item in Model) 
        {
            <div class="cursorPointer" onclick="GenerateLibraryReport('@item.ID');">
                <span>@item.Title</span>
            </div>
        }
    }
    else
    {
    <div>
        No reports found</div>
    }
}
