                            if (Model.SumAirSeconds == Model.PrevSumAirSeconds)
                            {
                            <div class="dashboardUpDiv" style="background: none">
                                Same</div>
                            }
                            else if (Model.SumAirSeconds > Model.PrevSumAirSeconds)
