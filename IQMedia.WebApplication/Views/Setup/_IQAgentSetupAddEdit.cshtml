                <input type="button" id="btnCancelTWRuleEdit" name="btnCancelTWRuleEdit"
                    value="Cancel" onclick="CancelIQAgentPopup('divEditTwitterRulePopup');" class="cancelButton"
                    style="margin: 0px;" />
                <input type="button" id="btnSubmitTWRuleEdit" name="btnSubmitTWRuleEdit"
                    value="Submit" onclick="SaveTwitterRules();" class="button" style="margin: 0px;" />
            </div>
            <div style="clear: both;">
            </div>
        </div>
        }
        <div class="modalBody">
            <div style="padding:10px 0px;">
                <span style="float:left; margin-right:10px; margin-top:5px;">Gnip Tag:</span>
                <input id="txtTwitterRuleTag" type="text" readonly="readonly" style="float:left; width:300px;" />                
