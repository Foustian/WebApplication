                {
                <a id="aAddTWRule" href="#" onclick="AddTwitterRuleSection();" style="float:right;">Add Rule Section</a>
                }
            </div>
            <div id="divTwitterRuleMsg" style="width:99%; text-align:center; color:Red; clear:both; margin-bottom:10px;"></div>
            <div id="divTwitterRules"></div>            
            <div>
                <ul class="ul-line-none twitterRuleOptions">
                    @{
                        string disabled = sessionInformation.IsExternalRuleEditor ? "" : "disabled=\"disabled\"";
                    <li><input type="checkbox" id="chkFollowers" name="chkTwitterRuleOption" @disabled />Followers &gt;15k</li>
                    <li><input type="checkbox" id="chkFollowing" name="chkTwitterRuleOption" @disabled />Following &gt;100</li>
                    <li><input type="checkbox" id="chkEnglish" name="chkTwitterRuleOption" @disabled />English Only</li>
                    <li><input type="checkbox" id="chkTimeZones" name="chkTwitterRuleOption" @disabled />US Time Zones Only</li>
                    }
                </ul>
            </div>
        </div>
    </div>
</div>

